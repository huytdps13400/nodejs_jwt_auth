{ "rewrites": [{ "source": "/(.*)", "destination": "/" }],  
        "builds": [
        {
            "src": "./server.js",
            "use": "@vercel/node"
        }
    ],
 "routes": [
      {
        "src": "/(.*)",
        "dest": "/app.js (It may be another name but this file should listen to the server)"
      }
    ]}
